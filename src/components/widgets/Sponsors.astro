import pat_oro_4 from '../../../src/img/ORO/YENEKAMU.svg';
import pat_oro_5 from '../../../src/img/ORO/FASE.svg';
import pat_plata_1 from '../../../src/img/PLATA/CABOSTYLE1.svg';
import pat_plata_2 from '../../../src/img/PLATA/REPSOL.svg';
