        </div>
        <h2 class="w-full text-center uppercase text-2xl text-white p-4">Plata</h2>
    </div>
    
    <!-- Hoyo -->
    <div class="py-16">
        <div class="grid grid-cols-2 md:grid-cols-5 py-4">
            <div class="grid place-items-center content-center">
                <img class="w-full max-w-96 h-full object-cover hover:-0" src={pat_hoyo_1.src} alt="am 1">
